{
    "name": "evodelavega/freshdesk-rest",
    "version": "1.0.0",
    "description": "A simple wrapper for the Freshdesk REST API. Freshdesk themselves are aware of this repo, and it is currently listed as the only third-party PHP wrapper",
    "keywords": ["Freshdesk", "Helpdesk", "Customer support", "REST", "API", "Wrapper", "JSON"],
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Elias Van Ootegem",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0" : {
            "Freshdesk" : "src"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*"
    },
    "require-dev": {
    	"phpunit/phpunit": "4.5.*"
    }
}
